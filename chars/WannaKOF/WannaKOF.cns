;---------------------------------------------------------------------------
; Stand
[Statedef 0]
type = S
physics = S
sprpriority = 0

[State 0, StopSnd]
type = StopSnd
trigger1 = time = 0
channel = 5

[State 0, 1]
type = ChangeAnim
trigger1 = Anim != 0 && Anim != 5
trigger2 = Anim = 5 && AnimTime = 0 ;Turn anim over
value = 0

[State 0, 2]
type = VelSet
trigger1 = Time = 0
y = 0

[State 0, 3] ;Stop moving if low velocity or 4 ticks pass
type = VelSet
trigger1 = abs(vel x) < Const(movement.stand.friction.threshold)
trigger2 = Time = 4
x = 0

[State 0, 4] ;Are you dead?
type = ChangeState
trigger1 = !alive
value = 5050
ctrl = 0

[State 0, 5] ;Wait, am I actually on the ground?
type = ChangeState
trigger1 = Pos y < 0
value = 50
ctrl = 1

[State 0, 6] ;Get down from there.
type = PosSet
trigger1 = Pos y > 0
y = 0

[State 0, 7] ;AM I EVEN REAL?
type = ChangeState
trigger1 = IsHelper
value = 12010
ctrl = 0

