[State 40, 2]
type = VarSet
Trigger1 = var(59) = 0
Trigger1 = command = "holdfwd"
Trigger2 = var(59) != 0
Trigger2 = EnemyNear,StateType != L || P2BodyDist X >= 30
Trigger2 = EnemyNear,MoveType = H || Random <= 333 || EnemyNear,StateType = L
